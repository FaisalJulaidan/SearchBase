  "type": "object",
  "properties": {
    "text": {"type": "string"},
    "action": {"enum": ["Show Solutions", "Go To Next Block", "Go To Specific Block"]},
    "fileTypes": {"type": "array", "items": {"type": "string"}},
    "blockToGoID": {"type": ["integer", "null"]}
    },
