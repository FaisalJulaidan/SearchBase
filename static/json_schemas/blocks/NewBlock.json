{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "new block schema",
  "type": "object",
  "properties": {
    "block": {
      "type": "object",
      "properties": {
        "type": { "enum": [ "Question", "User Input", "File Upload", "Solutions"] },
        "groupID": {"type": "integer"},
        "dataType": {"enum": ["None", "Email", "Telephone", "Contact Time", "Desired Salary", "Availability", "Desired Position", "Desired Position Years Exp", "Top Skill", "Top Skill Years Exp", "Resume", "Linkdin URL", "Desired Location", "Employment Type", "Desired Hourly Rate"]},
        "storeInDB": { "type": "boolean" },
        "isSkippable": { "type": "boolean" },
        "content": { "type": "object" }
      },
      "required": ["type","groupID", "dataType", "storeInDB", "content"],
      "additionalProperties": false
    }
  },
  "required": ["block"],
  "additionalProperties": false
}