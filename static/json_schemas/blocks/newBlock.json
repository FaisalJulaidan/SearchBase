{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "new block schema",
  "type": "object",
  "properties": {
    "block": {
      "type": "object",
      "properties": {
        "type": { "enum": [ "Question", "User Input", "File Upload", "Solutions"] },
        "groupID": {"type": "integer"},
        "dataCategoryID": {"type": ["integer", "null"]},
        "storeInDB": { "type": "boolean" },
        "isSkippable": { "type": "boolean" },
        "content": { "type": "object" }
      },
      "required": ["type","groupID", "dataCategoryID", "storeInDB", "content"],
      "additionalProperties": false
    }
  },
  "required": ["block"],
  "additionalProperties": false
}