{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "chatbot session schema",
  "type": "object",
  "properties": {
    "collectedData": {"type": "array", "items": {
      "type": "object",
      "properties":{
        "blockID": {"type": "integer"},
        "questionText": {"type": "string"},
        "dataType": {"enum": ["No Type", "Email", "Name", "Telephone", "Linkdin URL",
          "Degree", "Gender", "Resume", "Contact Time", "Availability", "Current Salary", "Desired Salary", "Desired Position", "Candidate Skills", "Years Exp", "Preferred Location", "Preferred Employment Type", "Desired Hourly Rate"
        , "Location", "NearbyStation", "Job Salary Offered", "Employment Type Offered", "Essential Skills", "Essential Years Exp",  "Job Description", "Job Availability"]},
        "input": {"type": "string"},
        "keywords": {"type": ["array","string"], "items": {"type": ["string"]}}
        },
      "required": ["blockID", "questionText", "dataType", "input", "keywords"],
      "additionalProperties": false
      }
    },
    "keywords": {"type": ["array","string"], "items": {"type": ["string"]}},
    "showTop": {"type": "integer"},
    "solutionsReturned": {"type": "integer"},
    "userType": {"enum": ["Unknown", "Candidate", "Client"]}
  },
  "required": ["collectedData", "keywords", "showTop", "solutionsReturned", "userType"],
  "additionalProperties": false
}
