{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "chatbot session schema",
  "type": "object",
  "properties": {
    "collectedData": {"type": "array", "items": {
      "type": "object",
      "properties":{
        "blockID": {"type": "integer"},
        "questionText": {"type": "string"},
        "dataType": {"enum": ["No Type", "Email", "Full Name", "Telephone", "Contact Time", "Desired Salary", "Availability", "Desired Position", "Desired Position Years Exp", "Top Skill", "Top Skill Years Exp", "Resume", "Linkdin URL", "Desired Location", "Employment Type", "Desired Hourly Rate"]},
        "input": {"type": "string"},
        "keywords": {"type": ["array","string"], "items": {"type": ["string"]}}
        },
      "required": ["blockID", "questionText", "dataType", "input", "keywords"],
      "additionalProperties": false
      }
    },
    "keywords": {"type": ["array","string"], "items": {"type": ["string"]}},
    "showTop": {"type": "integer"},
    "solutionsReturned": {"type": "integer"},
    "userType": {"enum": ["Unknown", "Candidate", "Client"]}
  },
  "required": ["collectedData", "keywords", "showTop", "solutionsReturned", "userType"],
  "additionalProperties": false
}
